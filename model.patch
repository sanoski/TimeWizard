-----------export const useTimesheetStore = create<TimesheetState>((set, get) => ({
-----------  entries: [],
-----------  lines: [],
-----------  // ... existing state
-----------
-----------  fetchEntries: async (weekEnding: string) => {
-----------    try {
-----------      set({ loading: true, error: null });
-----------      const entries = await DB.getEntriesByWeek(weekEnding);
